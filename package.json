{
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "start": "react-native start",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "code": "npx apollo service:download --endpoint=https://api.spacex.land/graphql graphql-schema.js",
    "schema:download": "apollo client:download-schema --endpoint=https://api.spacex.land/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "prestorybook": "rnstl"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src/components"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@react-native-async-storage/async-storage": "^1.13.0",
    "apollo": "^2.33.4",
    "expo": "~41.0.1",
